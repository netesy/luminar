        fn factorial(n) {
            if (n <= 1) {
                return 1;
            }
            return n * factorial(n - 1);
        }

        fn sum(a, b, c) {
            return a + b + c;
        }

        fn main() {
            var result1 = factorial(5);
            print(result1);

            var result2 = sum(1, 2, 3);
            print(result2);
        }

        main();